        function changecolor(item) {

            $("#" + item).parent().find("li").css("background-color", "transparent");
            $("#" + item).css("background-color", "#C9C9C9");
        }
        $(function () {

            $(document).mouseup(function (e) {
                var container = $(".search_main li");

                if (container.has(e.target).length === 0) {
                    container.hide();
                }
            });

            $(window).keydown(function (event) {
                if (event.keyCode == 13) {
                    event.preventDefault();
                    return false;
                }
            });


            $(".search").on('keyup click', function (event) {

                if (event.keyCode == 13 || event.which == 13) {
                    return false;
                }
                if ($(".search").val() != '') {
                    $(".search_main").find("li").css("display", "block");
                    $.ajax({
                        type: "POST",
                        url: "ws.asmx/GetBooks",
                        data: "{search:'" + $(".search").val() + "'}",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (msg) {
                            $(".login1").html('');
                            var li = '';
                            $.each(eval(msg.d), function (i, item) {
                                li += '<li id=li' + i + ' onmouseover=changecolor(this.id); style="list-style: none; padding: 10px 5px;;border-bottom: 1px solid #D6D6D6;text-decoration: none;">';
                                var a = ' onclick=insertTag(); href=' + 'BookDetail.aspx?BookID=' + item.BookID;
                                li += '<a style="text-decoration: none;color:green;"';
                                li += a;
                                li += '> ';

                                li += item.Title;
                                li += ' </a>';
                                li += '</li>';
                            })
                            if (eval(msg.d) == '') {
                                li += '<li  onmouseover=changecolor(this.id); style="list-style: none; padding: 10px 5px;;border-bottom: 1px solid #D6D6D6;text-decoration: none;">';
                                var a = 'onclick=insertTag(); href=#';
                                li += '<a style="text-decoration: none;color:green;"';
                                li += a;
                                li += '> ';

                                li += 'No Record found!';
                                li += ' </a>';
                                li += '</li>';


                                $(".login").append('<ul>' + li + '</ul>');
                            }
                            else {
                                $(".login").append('<ul>' + li + '</ul>');
                            }


                        }
                    });

                }
                else {
                    $(".search_main").find("li").css("display", "none");
                }



            });

            $(".hover").hover(function () {
                $(".hover").removeClass("selected");
                $(this).addClass("selected");



            },
            function () {
                $(".hover").removeClass("selected");


            });


            $(".s1").click(function () {

                $(".user1").removeClass("validate[required]");
                $(".user1").removeClass("validate[required,custom[onlyLetterSp]]");
                $(".user1").removeClass("validate[required,custom[email]]");
                $(".user1").removeClass("validate[required,equals[ctl00_ContentPlaceHolder1_txtPassword]]");

                $(":checkbox").removeClass("validate[required]");
                $(".area-box").removeClass("validate[required]");
                $(".tax-data").removeClass("validate[custom[onlyLetterSp]]");
                $(".tax-data").removeClass("validate[required,custom[email]]");
                $(".tax-data").removeClass("validate[required,custom[phone]]");





            });

            //$(".tax-data").addClass("validate[required,custom[email]]");
            //            $('.baby_bear').dropkick();
            //            $('.mama_bear').dropkick();
            //            $('.papa_bear').dropkick();

            //            $(".right_login li").hover(function () {
            //                $(".right_login li").removeClass("selected");
            //                $(this).addClass("selected");

            //            },
            //            function () {
            //                $(".right_login li").removeClass("selected");


            //            });

        });
        jQuery(document).ready(function () {
            // binds form submission and fields to the validation engine
            jQuery("#aspnetForm").validationEngine();



